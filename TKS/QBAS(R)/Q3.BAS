10 FOR i = 32 TO 99: a = i * i
20 m = INT(a / 100): n = a - m * 100
30 m1 = INT(m / 10): m2 = m - m1 * 10
40 n1 = INT(n / 10): n2 = n - n1 * 10
50 IF m1 = m2 AND n1 = n2 THEN PRINT a, i
60 NEXT
